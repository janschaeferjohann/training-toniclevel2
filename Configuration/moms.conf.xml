<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Configuration
	xmlns="http://www.kwsoft.de/moms/conf"
	collectRootFolderName="config-param:collectRootFolderName"
	splitSynchronously="false"
	timestampFormat="dd.MM.yyyy HH:mm:ss">
	<Stacks>
		<Stack
			allowToUpdateDocuments="true"
			callExtensionGroupEventsSeparately="true"
			continueOnDocumentError="true"
			documentDistributionMode="FULL_REST"
			maxDocumentCollectionDocumentCount="0"
			maxDocumentCollectionSheetCount="0"
			maxFirstDocumentCollectionDocumentCount="0"
			maxFirstDocumentCollectionSheetCount="0"
			maxParallelStacks="0"
			name="ARCHIVE"
			onlineStackSleepSeconds="0"
			processDocCollectionParallel="false"
			splitDocStatModAfterStackIR="FIN_STAT_OF_FIN_COL"
			tolerance="100"
			splitDocumentStatus="STACK_PROCESSING_SUCCESSFUL"
			persistControlJob="true"
			stackGroupID="ArchiveStacks"
			description="Stack for processing documents to be archived">
			<Select>
				<Equal
					logicalOperator="AND"
					parameterName="KW_MEDIUM"
					value="ARCHIVE" />
				<Equal
					logicalOperator="AND"
					value="WAIT_FOR_STACK_PROCESSING"
					parameterName="KW_STATUS" />
			</Select>
			<StackExtensions>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.ParameterSetter"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/ParameterSetter"
					name="set file path and file name">
					<StackExtension:ExtensionConfiguration
						can-run-parallel="false">
						<StackExtension:ParameterToSet
							parameter-name="ARCHIVE_PATH_NAME"
							parameter-type="Runtime-Parameter"
							runtime-parameter-data-type="Character"
							event="EVENT_TYPE_BEFORE_DOCUMENT_SELECT">
							<StackExtension:DynamicValue
								format="config-param:ARCHIVE_PATH_NAME">
							</StackExtension:DynamicValue>
						</StackExtension:ParameterToSet>
						<StackExtension:ParameterToSet
							parameter-name="ARCHIVE_FILE_NAME"
							parameter-type="Runtime-Parameter"
							event="EVENT_TYPE_DOCUMENT"
							runtime-parameter-data-type="Character">
							<StackExtension:DynamicValue
								format="%1$s/%4$tY%4$tm%4$td_PDF/%4$tY%4$tm%4$td_%4$tH%4$tM%4$tS_%2$s_%3$s_%5$s">
								<StackExtension:FormatArgument
									parameter-name="ARCHIVE_PATH_NAME"
									parameter-type="Runtime-Parameter"
									runtime-parameter-data-type="Character" />
								<StackExtension:FormatArgument
									parameter-name="KW_RUN_ID"
									parameter-type="M/OMS-Parameter" />
								<StackExtension:FormatArgument
									parameter-name="KW_SPLIT_ID"
									parameter-type="M/OMS-Parameter" />
								<StackExtension:FormatArgument
									parameter-name="KW_SPLIT_TIME"
									parameter-type="M/OMS-Parameter" />
								<StackExtension:FormatArgument
									parameter-name="KW_INPUT_ID"
									parameter-type="M/OMS-Parameter" />
							</StackExtension:DynamicValue>
						</StackExtension:ParameterToSet>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.renderer.RendererExtension"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/Renderer"
					name="render PDF">
					<StackExtension:ExtensionConfiguration
						destination="OMS-PDF"
						outputFilenameExtension="pdf"
						renderSingleDocuments="true"
						renderDocumentGroup="false"></StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.persistenter.PersistenterExtension"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/persistenter/PersistenterExtension"
					name="write file to storage">
					<StackExtension:ExtensionConfiguration>
						<StackExtension:PersistenterConfiguration
							doIfFileExists="re-name"
							nameOfResource="STACK_PRN"
							typeOfFile="*"
							unzip="false"
							event="EVENT_TYPE_DOCUMENT">
							<StackExtension:FileNameRoot>
								<StackExtension:FileNameParts>
									<StackExtension:FileNamePart
										number="1"
										parameter-type="Runtime-Parameter"
										parameter-name="ARCHIVE_FILE_NAME" />
								</StackExtension:FileNameParts>
							</StackExtension:FileNameRoot>
						</StackExtension:PersistenterConfiguration>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/FileExtension"
					className="de.kwsoft.moms.server.stack.extension.extensions.FileExtension"
					name="generate XML for archiving">
					<StackExtension:ExtensionConfiguration
						charSet="UTF-8"
						fileMode="DOCUMENT"
						formatUrl="mtx://Configuration/OMS/Resources/ArchiveXML.txt"
						locale="de_DE"
						outputType="xml"
						storeMode="FILE">
						<StackExtension:FileName format="%1$s.txt">
							<StackExtension:FormatArgument
								parameter-name="ARCHIVE_FILE_NAME"
								parameter-type="Runtime-Parameter"
								runtime-parameter-data-type="Character" />
						</StackExtension:FileName>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
			</StackExtensions>
		</Stack>
		<Stack
			allowToUpdateDocuments="true"
			callExtensionGroupEventsSeparately="true"
			continueOnDocumentError="true"
			documentDistributionMode="EQUAL"
			maxDocumentCollectionDocumentCount="0"
			maxDocumentCollectionSheetCount="0"
			maxFirstDocumentCollectionDocumentCount="0"
			maxFirstDocumentCollectionSheetCount="0"
			maxParallelStacks="0"
			name="LOCALPRINTING"
			onlineStackSleepSeconds="0"
			processDocCollectionParallel="false"
			splitDocStatModAfterStackIR="FIN_STAT_OF_FIN_COL"
			tolerance="100"
			removeStackRun="true"
			splitDocumentStatus="STACK_PROCESSING_SUCCESSFUL"
			stackGroupID="PrintStacks"
			description="PCL-rendered documents to be printed locally">
			<Select>
				<Equal
					logicalOperator="AND"
					parameterName="KW_MEDIUM"
					value="LOCALPRINTING" />
				<Equal
					logicalOperator="AND"
					value="WAIT_FOR_STACK_PROCESSING"
					parameterName="KW_STATUS" />
			</Select>
			<StackExtensions>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.ParameterSetter"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/ParameterSetter"
					name="set path name and file name">
					<StackExtension:ExtensionConfiguration
						can-run-parallel="false">
						<StackExtension:ParameterToSet
							parameter-name="LOCALPRINTING_PATH_NAME"
							parameter-type="Runtime-Parameter"
							runtime-parameter-data-type="Character"
							event="EVENT_TYPE_BEFORE_DOCUMENT_SELECT">
							<StackExtension:DynamicValue
								format="config-param:LOCALPRINTING_PATH_NAME">
							</StackExtension:DynamicValue>
						</StackExtension:ParameterToSet>
						<StackExtension:ParameterToSet
							parameter-name="LOCALPRINTING_FILE_NAME"
							parameter-type="Runtime-Parameter"
							event="EVENT_TYPE_START"
							runtime-parameter-data-type="Character">
							<StackExtension:DynamicValue
								format="%1$s/%2$s/%3$s">
								<StackExtension:FormatArgument
									parameter-name="LOCALPRINTING_PATH_NAME"
									parameter-type="Runtime-Parameter"
									runtime-parameter-data-type="Character" />
								<StackExtension:FormatArgument
									parameter-name="LOCALPRINTING_PRINTER"
									parameter-type="M/OMS-Parameter" />
								<StackExtension:FormatArgument
									parameter-name="KW_SPLIT_ID"
									parameter-type="M/OMS-Parameter" />
							</StackExtension:DynamicValue>
						</StackExtension:ParameterToSet>
						<StackExtension:ParameterToSet
							parameter-name="SRV_PRINTER_QUEUE"
							parameter-type="Runtime-Parameter"
							runtime-parameter-data-type="Character"
							event="EVENT_TYPE_BEFORE_DOCUMENT_SELECT">
							<StackExtension:DynamicValue
								format="config-param:SRV_PRINTER_QUEUE"></StackExtension:DynamicValue>
						</StackExtension:ParameterToSet>
						<StackExtension:ParameterToSet
							parameter-name="PRINTER_QUEUE"
							parameter-type="Runtime-Parameter"
							runtime-parameter-data-type="Character"
							event="EVENT_TYPE_DOCUMENT">
							<StackExtension:DynamicValue
								format="%1$s/%2$s">
								<StackExtension:FormatArgument
									parameter-name="SRV_PRINTER_QUEUE"
									parameter-type="Runtime-Parameter"
									runtime-parameter-data-type="Character" />
								<StackExtension:FormatArgument
									parameter-name="LOCALPRINTING_PRINTER"
									parameter-type="M/OMS-Parameter" />
							</StackExtension:DynamicValue>
						</StackExtension:ParameterToSet>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.renderer.RendererExtension"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/Renderer"
					name="render pcl file">
					<StackExtension:ExtensionConfiguration
						destination="OMS-PCL"
						renderInput="INTERPRET_GENERATED_MCJ"
						outputFilenameExtension="pcl"
						renderCollectFile="false"
						renderDocumentGroup="false"
						pageRepeatCountType="RUNTIME_PARAMETER"
						renderSingleDocuments="true">
						<StackExtension:KeyConfiguration>
							<StackExtension:JobKeyParameters>
								<StackExtension:KeyParameter
									parameter-name="LOCALPRINTING_USER_ID"
									parameter-type="M/OMS-Parameter"
									key-name="LOCALPRINTING_USER_ID" />
							</StackExtension:JobKeyParameters>
						</StackExtension:KeyConfiguration>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.persistenter.PersistenterExtension"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/persistenter/PersistenterExtension"
					name="write file to storage">
					<StackExtension:ExtensionConfiguration>
						<StackExtension:PersistenterConfiguration
							doIfFileExists="re-name"
							nameOfResource="STACK_PRN"
							typeOfFile="*"
							unzip="false"
							event="EVENT_TYPE_DOCUMENT">
							<StackExtension:FileNameRoot>
								<StackExtension:FileNameParts>
									<StackExtension:FileNamePart
										number="1"
										parameter-type="Runtime-Parameter"
										parameter-name="LOCALPRINTING_FILE_NAME" />
								</StackExtension:FileNameParts>
							</StackExtension:FileNameRoot>
						</StackExtension:PersistenterConfiguration>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.PrintResourceExtension"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/PrintResourceExtension">
					<StackExtension:ExtensionConfiguration
						resourceName="STACK_PRN"
						printerQueue="Printer_CUPS"
						queueNameParameterType="Runtime-Parameter"
						queueNameParameterName="PRINTER_QUEUE">
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
			</StackExtensions>
		</Stack>
		<Stack
			allowToUpdateDocuments="true"
			callExtensionGroupEventsSeparately="true"
			continueOnDocumentError="true"
			documentDistributionMode="EQUAL"
			maxDocumentCollectionDocumentCount="0"
			maxDocumentCollectionSheetCount="0"
			maxFirstDocumentCollectionDocumentCount="0"
			maxFirstDocumentCollectionSheetCount="0"
			maxParallelStacks="0"
			name="CENTRALPRINTING"
			onlineStackSleepSeconds="0"
			processDocCollectionParallel="false"
			splitDocStatModAfterStackIR="FIN_STAT_OF_FIN_COL"
			tolerance="100"
			description="Stack for rendering document collections to AFP format.
One file for each country of destination is produced."
			removeStackRun="true"
			splitDocumentStatus="STACK_PROCESSING_SUCCESSFUL"
			stackGroupID="PrintStacks"
			groupNameToLimitDocumentCollection="CountryCode">
			<Select>
				<Equal
					logicalOperator="AND"
					parameterName="KW_MEDIUM"
					value="CENTRALPRINTING" />
				<Equal
					logicalOperator="AND"
					value="WAIT_FOR_STACK_PROCESSING"
					parameterName="KW_STATUS" />
			</Select>
			<StackExtensions>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.ParameterSetter"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/ParameterSetter"
					name="set file path and file name">
					<StackExtension:ExtensionConfiguration
						can-run-parallel="false">
						<StackExtension:ParameterToSet
							parameter-name="CENTRALPRINTING_PATH_NAME"
							parameter-type="Runtime-Parameter"
							runtime-parameter-data-type="Character"
							event="EVENT_TYPE_START">
							<StackExtension:DynamicValue
								format="config-param:CENTRALPRINTING_PATH_NAME">
							</StackExtension:DynamicValue>
						</StackExtension:ParameterToSet>
						<StackExtension:ParameterToSet
							parameter-name="CENTRALPRINTING_FILE_NAME"
							parameter-type="Runtime-Parameter"
							event="EVENT_TYPE_DOCUMENT_COLLECTION_START"
							runtime-parameter-data-type="Character">
							<StackExtension:DynamicValue
								format="%1$s/%3$tY%3$tm%3$td_AFP/%3$tY%3$tm%3$td_%3$tH%3$tM%3$tS_%2$s_%4$s.afp">
								<StackExtension:FormatArgument
									parameter-name="CENTRALPRINTING_PATH_NAME"
									parameter-type="Runtime-Parameter"
									runtime-parameter-data-type="Character" />
								<StackExtension:FormatArgument
									parameter-name="KW_RUN_ID"
									parameter-type="M/OMS-Parameter" />
								<StackExtension:FormatArgument
									parameter-name="START_TIME_DATE"
									parameter-type="Runtime-Parameter" />
								<StackExtension:FormatArgument
									parameter-name="RECIPIENT_COUNTRY_CODE"
									parameter-type="M/OMS-Parameter" />
							</StackExtension:DynamicValue>
						</StackExtension:ParameterToSet>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.GroupValuesSaver"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/GroupValuesSaver">
					<StackExtension:ExtensionConfiguration
						setGroupId="false"
						setGroupDocumentCount="false"
						setGroupPosition="false"
						setGroupSheetCount="true"
						groupName="Envelope" />
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.marking.MarkingExtension"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/Marking"
					name="Datamatrix">
					<StackExtension:ExtensionConfiguration>
						<StackExtension:MarkingConfiguration>
							<StackExtension:Marking
								name="QR Code"
								length="23"
								pageDirection="FORWARD"
								paddingChar="'0'">
								<StackExtension:PagePositionConfiguration
									sheetFrontPages="true"></StackExtension:PagePositionConfiguration>
								<StackExtension:BarcodeMarkingFormat
									name="QR Code">
									<StackExtension:BarcodeMetadata
										code-type="QR"
										code-size="0"
										width="1200"
										height="1200" />
									<StackExtension:BarcodeLocationPortrait
										position-x="14173"
										position-y="235843" />
									<StackExtension:BarcodeLocationLandscape
										position-x="56693"
										position-y="56693" />
								</StackExtension:BarcodeMarkingFormat>
								<StackExtension:TextMarkingFormat
									name="Klarschrift"
									position="1"
									length="23">
									<StackExtension:Text
										fontSize="7000"
										fontName="STANDARD"
										fontStyle="0" />
									<StackExtension:PageOperationLocationPortrait
										position-x="14173"
										position-y="226772"
										orientation="90" />
									<StackExtension:PageOperationLocationLandscape />
								</StackExtension:TextMarkingFormat>
								<StackExtension:Command
									name="KW Kennzeichen"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.ConstantValue"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/ConstantValue"
									position="1"
									length="2"
									encoding="STRING">
									<Command:ExtensionConfiguration
										value="KW" />
								</StackExtension:Command>
								<StackExtension:Command
									name="Minus"
									position="3"
									length="1"
									encoding="STRING"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.ConstantValue"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/ConstantValue">
									<Command:ExtensionConfiguration
										value="-" />
								</StackExtension:Command>
								<StackExtension:Command
									name="Run ID"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.MOMSParameter"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/MOMSParameter"
									paddingChar="'0'"
									position="4"
									length="6"
									encoding="STR_DECIMAL">
									<Command:ExtensionConfiguration
										parameterName="KW_RUN_ID"
										formatPattern="%06d" />
								</StackExtension:Command>
								<StackExtension:Command
									name="Minus B"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.ConstantValue"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/ConstantValue"
									position="10"
									length="1"
									encoding="STRING">
									<Command:ExtensionConfiguration
										value="-" />
								</StackExtension:Command>
								<StackExtension:Command
									name="Nummer des Umschlags"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.GroupOfDocumentCollection"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/GroupOfDocumentCollection"
									position="11"
									length="4"
									encoding="STR_DECIMAL"
									paddingChar="'0'">
									<Command:ExtensionConfiguration
										groupName="Envelope" />
								</StackExtension:Command>
								<StackExtension:Command
									name="Minus C"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.ConstantValue"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/ConstantValue"
									position="15"
									length="1"
									encoding="STRING">
									<Command:ExtensionConfiguration
										value="-" />
								</StackExtension:Command>
								<StackExtension:Command
									name="Blatt im Umschlag"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.SheetOfDocumentGroup"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/SheetOfDocumentGroup"
									position="16"
									length="4"
									encoding="STR_DECIMAL"
									paddingChar="'0'">
									<Command:ExtensionConfiguration
										groupName="Envelope" />
								</StackExtension:Command>
								<StackExtension:Command
									name="Minus D"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.ConstantValue"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/ConstantValue"
									position="20"
									length="1"
									encoding="STRING">
									<Command:ExtensionConfiguration
										value="-" />
								</StackExtension:Command>
								<StackExtension:Command
									name="Anzahl Blätter im Umschlag"
									className="de.kwsoft.moms.server.stack.extension.extensions.marking.commands.MOMSParameter"
									xmlns:Command="http://www.kwsoft.de/moms/ext/stack/marking/command/MOMSParameter"
									position="21"
									length="3"
									encoding="STR_DECIMAL"
									paddingChar="'0'">
									<Command:ExtensionConfiguration
										parameterName="KW_GROUP_SHEETS" />
								</StackExtension:Command>
							</StackExtension:Marking>
						</StackExtension:MarkingConfiguration>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.renderer.RendererExtension"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/Renderer"
					name="render AFP">
					<StackExtension:ExtensionConfiguration
						destination="OMS-AFP"
						renderInput="INTERPRET_GENERATED_MCJ"
						outputFilenameExtension="afp"
						renderCollectFile="true"
						renderDocumentGroup="false"
						pageRepeatCountType="RUNTIME_PARAMETER"
						renderSingleDocuments="false"
						coverpagePosition="NONE"
						pageTemplateNameType="URL">
						<StackExtension:KeyConfiguration>
							<StackExtension:DocumentKeyParameters>
								<StackExtension:KeyParameter
									parameter-name="RECIPIENT_COUNTRY_CODE"
									parameter-type="M/OMS-Parameter"
									key-name="LKZ" />
								<StackExtension:KeyParameter
									parameter-name="RECIPIENT_COUNTRY_CODE"
									parameter-type="M/OMS-Parameter"
									key-name="PLZ" />
							</StackExtension:DocumentKeyParameters>
						</StackExtension:KeyConfiguration>
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
				<StackExtension
					className="de.kwsoft.moms.server.stack.extension.extensions.CollectExtension"
					xmlns:StackExtension="http://www.kwsoft.de/moms/ext/stack/Collect"
					name="deposit AFP">
					<StackExtension:ExtensionConfiguration
						outputFilenameExtension="afp"
						fileNameParam="CENTRALPRINTING_FILE_NAME"
						pageRepeatCountType="CONSTANT"
						coverpagePosition="END"
						pageTemplate="mtx://Configuration/OMS/Resources/Collection_CoverSheet.mtx"
						endPageTemplate="mtx://Configuration/OMS/Resources/Collection_CoverSheet.mtx">
						<StackExtension:PlaceHolderParameter
							parameterType="STACK_PARAMETER"
							placeholderName="PRINTFILE_DOCUMENT_COUNT"
							parameterNameOrValue="PRINTFILE_DOCUMENT_COUNT" />
						<StackExtension:PlaceHolderParameter
							parameterType="STACK_PARAMETER"
							placeholderName="PRINTFILE_SHEET_COUNT"
							parameterNameOrValue="PRINTFILE_SHEET_COUNT" />
						<StackExtension:PlaceHolderParameter
							parameterType="DOCUMENT_PARAMETER"
							placeholderName="RUN_ID"
							parameterNameOrValue="KW_RUN_ID" />
						<StackExtension:PlaceHolderParameter
							parameterType="RUNTIME_PARAMETER"
							placeholderName="STACK_NAME"
							parameterNameOrValue="STACK_NAME" />
						<StackExtension:PlaceHolderParameter
							parameterType="RUNTIME_PARAMETER"
							placeholderName="STACK_DSC"
							parameterNameOrValue="STACK_DESCRIPTION" />
						<StackExtension:PlaceHolderParameter
							parameterType="RUNTIME_PARAMETER"
							placeholderName="START_TIME_DATE"
							parameterNameOrValue="START_TIME_DATE" />
						<StackExtension:PlaceHolderParameter
							parameterType="DOCUMENT_PARAMETER"
							placeholderName="LKZ"
							parameterNameOrValue="RECIPIENT_COUNTRY_CODE" />
					</StackExtension:ExtensionConfiguration>
				</StackExtension>
			</StackExtensions>
			<Groups>
				<Group
					changeAsSortParam="true"
					breakAllowed="false"
					sortParameter="RECIPIENT_COUNTRY_CODE"
					resetAtCollectionStart="false"
					groupName="CountryCode" />
				<Group
					changeAsSortParam="true"
					breakAllowed="false"
					sortParameter="RECIPIENT_ID"
					resetAtCollectionStart="false"
					groupName="Envelope" />
			</Groups>
			<SortParameters>
				<SortParameter
					order="asc"
					name="RECIPIENT_COUNTRY_CODE" />
				<SortParameter
					order="asc"
					name="RECIPIENT_ID" />
			</SortParameters>
		</Stack>
		<Stack
			allowToUpdateDocuments="true"
			callExtensionGroupEventsSeparately="true"
			continueOnDocumentError="false"
			documentDistributionMode="FULL_REST"
			maxDocumentCollectionDocumentCount="0"
			maxDocumentCollectionSheetCount="0"
			maxFirstDocumentCollectionDocumentCount="0"
			maxFirstDocumentCollectionSheetCount="0"
			maxParallelStacks="2"
			name="EMAIL"
			onlineStackSleepSeconds="0"
			processDocCollectionParallel="true"
			splitDocStatModAfterStackIR="FIN_STAT_OF_FIN_COL"
			tolerance="100"
			splitDocumentStatus="STACK_PROCESSING_SUCCESSFUL">
			<Select>
				<In
					logicalOperator="AND"
					parameterName="KW_MEDIUM">
					<Value>EMAIL</Value>
					<Value>EMAILATTACHMENT</Value>
				</In>
				<Equal
					logicalOperator="AND"
					value="WAIT_FOR_STACK_PROCESSING"
					parameterName="KW_STATUS" />
